{
  "name": "3d-tiles-validator",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "description": "Tools for validating 3D Tiles tilesets.",
  "keywords": [
    "3D Tiles"
  ],
  "homepage": "https://github.com/CesiumGS/3d-tiles-validator/",
  "repository": {
    "type": "git",
    "url": "https://github.com/CesiumGS/3d-tiles-validator.git"
  },
  "bugs": {
    "url": "https://github.com/CesiumGS/3d-tiles-validator/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "dependencies": {
    "archiver": "^5.3.1",
    "better-sqlite3": "^7.5.3",
    "cesium": "^1.97.0",
    "gltf-validator": "^2.0.0-dev.3.9",
    "minimatch": "^5.1.0",
    "node-stream-zip": "^1.10.1",
    "yargs": "^17.5.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.19.24",
    "@microsoft/api-extractor": "^7.33.6",
    "@tsconfig/node16": "^1.0.3",
    "@types/archiver": "^5.3.1",
    "@types/better-sqlite3": "^7.6.2",
    "@types/jasmine": "^4.0.3",
    "@types/minimatch": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-cesium": "^9.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jasmine": "^4.3.0",
    "jasmine-node": "^3.0.0",
    "mkdirp": "^1.0.4",
    "nyc": "^15.1.0",
    "prettier": "2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "eslint": "eslint \"./**/*.*s\" --cache",
    "build": "npx tsc",
    "start": "npx ts-node src/main.ts",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=specs/jasmine.json",
    "coverage": "nyc npm run test",
    "prettier-check": "prettier --check \"**/*\"",
    "prettier": "prettier --write \"**/*\"",
    "generate-third-party": "node generateThirdParty.js",
    "docs": "mkdirp etc && npx tsc && api-extractor run --config api-extractor.jsonc --local --verbose && api-documenter markdown -i build/api-extractor -o build/docs"
  }
}
