{
  "extends": "@tsconfig/node16/tsconfig.json",
  "compilerOptions": {
    "outDir": "build",
    "strict": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "noImplicitOverride": true,
    "declaration": true,
    "declarationMap": true,
    "lib": [
      "dom"
    ]
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
